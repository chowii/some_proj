<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="UserAvatarView">
        <attr name="avatar" format="integer" />
        <attr name="name" format="string" localization="suggested" />
        <attr name="role" format="string" localization="suggested" />
        <attr name="name_visibility" value="visible">
            <enum name="visible" value="0x0" />
            <enum name="invisible" value="0x4" />
            <enum name="gone" value="0x8" />
        </attr>
        <attr name="role_visibility" value="visible">
            <enum name="visible" value="0x0" />
            <enum name="invisible" value="0x4" />
            <enum name="gone" value="0x8" />
        </attr>
    </declare-styleable>
    <declare-styleable name="LabeledValueField">
        <attr name="title" format="string" localization="suggested" />
        <attr name="value" format="string" localization="suggested" />
        <attr name="hint" format="string" localization="suggested" />
        <attr name="inputType" value="text">
            <enum name="text" value="1" />
            <enum name="number" value="2" />
            <enum name="phone" value="3" />
            <enum name="numberDecimal" value="2002" />
            <enum name="textEmailAddress" value="21" />
        </attr>
    </declare-styleable>
</resources>